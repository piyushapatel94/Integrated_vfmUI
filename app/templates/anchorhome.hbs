{{!--{{partial 'steplist'}}--}}
{{!--
 {{models-table data=model columns=columns anchorprogram='anchorprogram'  }}--}}


<table >
    <thead>
      <tr>
        <th>Program</th>
        <th>Date</th>
        <th>Status</th>
         <th>close</th>
      </tr>
    </thead>
    <tbody>
     
     {{#each message as |item|}}
      <tr>
      <td>  {{item.programid}} </td>
      <td>  {{item.date}}</td>
      <td>  <button {{action 'anchorprogram' item.status item.programid item}}>{{item.status}}</button></td>
  
     <td>  <button {{action 'close' item}}>Close</button></td>
      </tr>
      {{/each}}
     
     
    </tbody>
  </table>